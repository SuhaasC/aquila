{
    "name": "aquilastrat/website",
    "description": "Aquilastrat - Strategic Partners for Leaders Who Move Markets",
    "type": "project",
    "keywords": ["strategy", "consulting", "data-management", "nextjs"],
    "homepage": "https://aquilastrat.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Suhaas Chittapurath",
            "email": "suhaas@aquilastrat.com",
            "role": "Founder"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Aquilastrat\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "echo 'Aquilastrat website deployed successfully!'",
            "echo 'Next.js application ready for hosting'"
        ],
        "deploy": [
            "echo 'Starting deployment...'",
            "echo 'Website files are ready for Hostinger'"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
